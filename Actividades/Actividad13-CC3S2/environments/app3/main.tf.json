{
    "resource": [
        {
            "null_resource": [
                {
                    "local_server-app3": [
                        {
                            "provisioner": [
                                {
                                    "local-exec": {
                                        "command": "echo 'Arrancando servidor ${var.name} en red ${var.network}:${var.port} (autenticado)'"
                                    }
                                }
                            ],
                            "triggers": {
                                "api_key_hash": "${md5(var.api_key)}",
                                "name": "${var.name}",
                                "network": "${var.network}",
                                "port": "${var.port}"
                            }
                        }
                    ]
                }
            ]
        }
    ]
}